[32m+[m[32mdef is_happy(n):[m
[32m+[m[32m    if n == 1:[m
[32m+[m[32m        return True[m
[32m+[m
[32m+[m[32m    if n <= 4:[m
[32m+[m[32m        return False[m
[32m+[m
[32m+[m[32m    sums = 0[m
[32m+[m
[32m+[m[32m    while n / 10 > 0:[m
[32m+[m[32m        sums += (n % 10) ** 2[m
[32m+[m[32m        n //= 10[m
[32m+[m
[32m+[m[32m    return is_happy(sums)[m
[32m+[m
[32m+[m
[32m+[m[32mprint(is_happy())[m
