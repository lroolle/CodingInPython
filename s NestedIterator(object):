[32m+[m[32mdef count_ones_r(num):[m
[32m+[m[32m    if num < 2:[m
[32m+[m[32m        return num[m
[32m+[m[32m    print(num)[m
[32m+[m
[32m+[m[32m    return num % 2 + count_ones_r(num // 2)[m
[32m+[m
[32m+[m[32mprint("xx_x", count_ones_r(125))[m
