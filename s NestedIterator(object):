[36m@@ -0,0 +1,14 @@[m
[32m+[m[32mdef yield_generator(n):[m[41m[m
[32m+[m[32m    l = range(n)[m[41m[m
[32m+[m[32m    for i in l:[m[41m[m
[32m+[m[32m        yield i * i[m[41m[m
