[32m+[m[32m# bubble_sort.py[m
[32m+[m
[32m+[m
[32m+[m[32mdef bubble_sort(a):[m
[32m+[m[32m    for i in range(len(a) - 1):[m
[32m+[m[32m        for j in range(1, len(a) - i):[m
[32m+[m[32m            if a[j - 1] > a[j]:[m
[32m+[m[32m                a[j - 1], a[j] = a[j], a[j - 1][m
[32m+[m
[32m+[m[32m    return a[m
